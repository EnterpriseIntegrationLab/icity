###  http://ontology.eil.utoronto.ca/icity/Trip/Tour
:Tour rdf:type owl:Class ;
      rdfs:subClassOf :Trip ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :startLoc ;
                        owl:allValuesFrom [ rdf:type owl:Restriction ;
                                            owl:onProperty [ owl:inverseOf :endLoc
                                                           ] ;
                                            owl:hasSelf "true"^^xsd:boolean
                                          ]
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://ontology.eil.utoronto.ca/icity/Activity/hasSubactivity> ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Trip
                      ] ;
      <http://purl.org/dc/elements/1.1/description> """A sequence of Trips made by one Person.
A Tour starts and ends at the same Location.""" ;
      rdfs:comment "Future work: define a new property to enforce the same start and end location constraint." .


###  http://ontology.eil.utoronto.ca/icity/Trip/Trip
:Trip rdf:type owl:Class ;
      rdfs:subClassOf <http://ontology.eil.utoronto.ca/icity/Activity/Activity> ,
                      :TripOntologyThing ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasDriver ;
                        owl:allValuesFrom <http://ontology.eil.utoronto.ca/icity/Change/Manifestation>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasPassenger ;
                        owl:allValuesFrom <http://ontology.eil.utoronto.ca/icity/Change/Manifestation>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :viaMode ;
                        owl:allValuesFrom <http://ontology.eil.utoronto.ca/icity/TransportationSystem/Mode>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :viaVehicle ;
                        owl:allValuesFrom <http://ontology.eil.utoronto.ca/icity/Vehicle/Vehicle>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :accessesArc ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass <http://ontology.eil.utoronto.ca/icity/TransportationSystem/Arc>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :accessesNetwork ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass <http://ontology.eil.utoronto.ca/icity/TransportationSystem/Network>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :occursOn ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass <http://ontology.eil.utoronto.ca/icity/TransportationSystem/TransportationComplex>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :endLoc ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass <http://www.opengis.net/ont/geosparql#Feature>
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :startLoc ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass <http://www.opengis.net/ont/geosparql#Feature>
                      ] ;
      <http://purl.org/dc/elements/1.1/description> """A Trip describes movement of a Persons from one location to another via some Mode(s).
A Trip is made by a Person to facilitate participation in some Activity.
A Trip starts at some Location and ends at some Location.
A Trip occurs during some Interval.
A Trip occurs in some Network(s).
A Trip occurs via some Arc(s).
A Trip occurs on some Transportation Complex. (e.g. a road or a rail)
A Trip contains some Trip Segments.
A Trip may incur some cost (monetary or otherwise).""" .


