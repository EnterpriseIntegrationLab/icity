}, {
  "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/MereologyOntologyProperty",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://purl.org/dc/elements/1.1/description" : [ {
    "@value" : "Created for organizational purposes, to identify properties defined in the Mereology ontology."
  } ]
}, {
  "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/componentOf",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://purl.org/dc/elements/1.1/description" : [ {
    "@value" : "Something may be a Component of some other thing\nMore specifically, something may be a immediate component of something; in other words, if x is an immediate component of y, then there does not exist any other object that is a component of y and has x as a component.\nComponent-of is transitive. Immediate component-of is not transitive.\nImmediate component-of is a subproperty of component-of."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subPropertyOf" : [ {
    "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/properPartOf"
  } ],
  "http://www.w3.org/2002/07/owl#inverseOf" : [ {
    "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/hasComponent"
  } ]
}, {
  "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/containedIn",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://purl.org/dc/elements/1.1/description" : [ {
    "@value" : "Something may be contained-in some other thing; more specifically it may be immediately contained in something.\nContainment is transitive. Immediate containment is not transitive.\nImmediate containment is a subproperty of containment."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subPropertyOf" : [ {
    "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/MereologyOntologyProperty"
  } ],
  "http://www.w3.org/2002/07/owl#inverseOf" : [ {
    "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/contains"
  } ]
}, {
  "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/contains",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#subPropertyOf" : [ {
    "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/MereologyOntologyProperty"
  } ]
}, {
  "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/hasComponent",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#subPropertyOf" : [ {
    "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/hasProperPart"
  } ]
}, {
  "@id" : "http://ontology.eil.utoronto.ca/icity/Mereology/hasProperPart",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
