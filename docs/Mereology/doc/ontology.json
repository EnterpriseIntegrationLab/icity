  "http://purl.org/dc/elements/1.1/description" : [ {
    "@value" : "Ontology to capture concepts related to parthood.\nThe Mereology Ontology extends beyond classical mereology to cover parthood, but also component-hood and containment."
  } ],
  "http://purl.org/dc/elements/1.1/issued" : [ {
    "@value" : "November 29, 2016"
  } ],
