    <!-- http://ontology.eil.utoronto.ca/cdm/Person/PersonPD -->


    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/cdm/Person/PersonPD">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/hasManifestation"/>
                        <owl:someValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Person/Person"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/hasManifestation"/>
                        <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Person/Person"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/TimeVaryingConcept"/>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/cdm/Person/PersonOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/existsAt"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#Interval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Person/hasPersonID"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Person/PersonId"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Person/hasSex"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/cdm/Person/Sex"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://schema.org/birthDate"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#Instant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Person changes over time, the entire entity of a Person is captured by the PersonPD (&quot;person perdurant&quot;) class.</dc:description>
    </owl:Class>
    


