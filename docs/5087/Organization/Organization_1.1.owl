    <!-- http://ontology.eil.utoronto.ca/cdm/Organization/BusinessEstablishment -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/cdm/Organization/BusinessEstablishment">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/manifestationOf"/>
                        <owl:someValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Organization/BusinessEstablishmentPD"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/manifestationOf"/>
                        <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Organization/BusinessEstablishmentPD"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/Manifestation"/>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/cdm/Organization/OrgOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://schema.org/address"/>
                <owl:allValuesFrom rdf:resource="http://schema.org/PostalAddress"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/existsAt"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/SpatialLoc/1.0/hasLocation"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/cdm/SpatialLoc/1.0/SpatialFeature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Business Establishment: A Business establishment is a physical location where a Firm conducts business.
A Business Establishment has a Location and may have an address.</dc:description>
    </owl:Class>
    


