    <!-- http://ontology.eil.utoronto.ca/5087/PublicTransit/AccessMethod -->


    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/5087/PublicTransit/AccessMethod">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/5087/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/5087/PublicTransit/hasMonetaryCost"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/5087/MonetaryValue/1.0/MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/5087/PublicTransit/validFor"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/ISO21972/iso21972#Quantity"/>
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#DurationDescription"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An Access Method is the means of access to a Line
An AccessMethod has a Monetary Value.
An AccessMethod may be valid for a specific distance or time.</dc:description>
    </owl:Class>
    


