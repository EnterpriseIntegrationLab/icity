    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/cdm/Building/BuildingPD">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/hasManifestation"/>
                        <owl:someValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Building/Building"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/hasManifestation"/>
                        <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Building/Building"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/cdm/Building/BuildingOntologyThing"/>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/TimeVaryingConcept"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/1.1/existsAt"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#Interval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Building may change over time, BuildingPD represents the entire process of some building entity (i.e. the Building perdurant).</dc:description>
    </owl:Class>
    


