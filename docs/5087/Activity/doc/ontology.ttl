###  http://ontology.eil.utoronto.ca/5087/Activity/ActivityOntologyProperty
:ActivityOntologyProperty rdf:type owl:ObjectProperty ;
                          <http://purl.org/dc/elements/1.1/description> "Added for organizational purposes, to identify all properties defined in the Activity ontology." .


###  http://ontology.eil.utoronto.ca/5087/Activity/achievedAt
:achievedAt rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :ActivityOntologyProperty ;
            rdfs:domain :State ;
            rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
            owl:propertyChainAxiom ( [ owl:inverseOf :effectOf
                                     ]
                                     :endOf
                                   ) ,
                                   ( [ owl:inverseOf :hasDecomp
                                     ]
                                     <http://www.wurvoc.org/vocabularies/om-1.8/conjunctiveAchievedAt>
                                     <http://www.w3.org/2006/time#intervalDuring>
                                   ) ,
                                   ( [ owl:inverseOf :preconditionOf
                                     ]
                                     :beginOf
                                   ) .


###  http://ontology.eil.utoronto.ca/5087/Activity/beginOf
:beginOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :ActivityOntologyProperty ;
         rdfs:domain :Activity ;
         rdfs:range <http://www.w3.org/2006/time#Instant> ;
         owl:propertyChainAxiom ( :occursAt
                                  <http://www.w3.org/2006/time#hasBeginning>
                                ) .


###  http://ontology.eil.utoronto.ca/5087/Activity/causedBy
:causedBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :ActivityOntologyProperty ;
          owl:inverseOf :causes .


###  http://ontology.eil.utoronto.ca/5087/Activity/causes
:causes rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :ActivityOntologyProperty .


###  http://ontology.eil.utoronto.ca/5087/Activity/effectOf
:effectOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :ActivityOntologyProperty ;
          owl:inverseOf :hasEffect .


###  http://ontology.eil.utoronto.ca/5087/Activity/enabledBy
:enabledBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :ActivityOntologyProperty ;
           owl:inverseOf :enables .


###  http://ontology.eil.utoronto.ca/5087/Activity/enables
:enables rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :ActivityOntologyProperty .


###  http://ontology.eil.utoronto.ca/5087/Activity/endOf
:endOf rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :ActivityOntologyProperty ;
       rdfs:domain :Activity ;
       rdfs:range <http://www.w3.org/2006/time#Instant> ;
       owl:propertyChainAxiom ( :occursAt
                                <http://www.w3.org/2006/time#hasEnd>
                              ) .


###  http://ontology.eil.utoronto.ca/5087/Activity/hasDecomp
:hasDecomp rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasSubstate ;
           rdfs:domain :State ;
           rdfs:range :State .


###  http://ontology.eil.utoronto.ca/5087/Activity/hasEffect
:hasEffect rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :causedBy ;
           rdfs:domain :Activity ;
           rdfs:range :State .


###  http://ontology.eil.utoronto.ca/5087/Activity/hasPrecondition
:hasPrecondition rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :enabledBy ;
                 owl:inverseOf :preconditionOf ;
                 rdfs:domain :Activity ;
                 rdfs:range :State .


###  http://ontology.eil.utoronto.ca/5087/Activity/hasResource
:hasResource rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :ActivityOntologyProperty ;
             owl:inverseOf :resourceOf ;
             rdfs:range <http://ontology.eil.utoronto.ca/5087/Change/Manifestation> .


###  http://ontology.eil.utoronto.ca/5087/Activity/hasSubactivity
:hasSubactivity rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :ActivityOntologyProperty .


###  http://ontology.eil.utoronto.ca/5087/Activity/hasSubstate
:hasSubstate rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :ActivityOntologyProperty ;
             rdf:type owl:TransitiveProperty .


###  http://ontology.eil.utoronto.ca/5087/Activity/occursAfter
:occursAfter rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :ActivityOntologyProperty ;
             owl:inverseOf :occursBefore .


###  http://ontology.eil.utoronto.ca/5087/Activity/occursAt
:occursAt rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :ActivityOntologyProperty ;
          rdfs:range <http://www.w3.org/2006/time#TemporalEntity> .


###  http://ontology.eil.utoronto.ca/5087/Activity/occursBefore
:occursBefore rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :ActivityOntologyProperty ;
              rdf:type owl:TransitiveProperty ;
              rdfs:domain :Activity ;
              rdfs:range :Activity ;
              rdfs:comment "An activity occursBefore another if its endOf instant is before the beginOf instant of the other activity; the occursBefore relation is transitive. We cannot define this semantics in OWL, though it could be captured in future work by an extension with rules." .


###  http://ontology.eil.utoronto.ca/5087/Activity/occursDirectlyAfter
:occursDirectlyAfter rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :occursAfter ;
                     owl:inverseOf :occursDirectlyBefore .


###  http://ontology.eil.utoronto.ca/5087/Activity/occursDirectlyBefore
:occursDirectlyBefore rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :occursBefore ;
                      rdfs:domain :Activity ;
                      rdfs:range :Activity ;
                      rdfs:comment "An activity occursDirectlyBefore another if it occursAt an interval that meets the interval of the other activity. We cannot define this semantics in OWL, though it could be captured by an extension with rules." .


###  http://ontology.eil.utoronto.ca/5087/Activity/preconditionOf
:preconditionOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :ActivityOntologyProperty .


###  http://ontology.eil.utoronto.ca/5087/Activity/resourceOf
:resourceOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :ActivityOntologyProperty .


###  http://ontology.eil.utoronto.ca/5087/Activity/stateTypeOf
:stateTypeOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :ActivityOntologyProperty ;
             rdfs:domain :TerminalState ;
             rdfs:range <http://ontology.eil.utoronto.ca/5087/Change/Manifestation> .


###  http://ontology.eil.utoronto.ca/5087/Change/ChangeOntologyProperty
<http://ontology.eil.utoronto.ca/5087/Change/ChangeOntologyProperty> rdf:type owl:ObjectProperty ;
                                                                     rdfs:comment "Added for organizational purposes to group properties defined in the Change Ontology." .


###  http://ontology.eil.utoronto.ca/5087/Change/existsAt
<http://ontology.eil.utoronto.ca/5087/Change/existsAt> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://ontology.eil.utoronto.ca/5087/Change/ChangeOntologyProperty> ;
                                                       rdfs:domain <http://ontology.eil.utoronto.ca/5087/Change/Manifestation> ;
                                                       rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
                                                       <http://purl.org/dc/elements/1.1/description> "Objects (e.g. perdurants and their manifestations) exist at some time interval or time point." .


###  http://ontology.eil.utoronto.ca/5087/Change/hasNextManifestation
<http://ontology.eil.utoronto.ca/5087/Change/hasNextManifestation> rdf:type owl:ObjectProperty ;
                                                                   rdfs:subPropertyOf <http://ontology.eil.utoronto.ca/5087/Change/ChangeOntologyProperty> ;
                                                                   rdf:type owl:InverseFunctionalProperty ;
                                                                   rdfs:domain <http://ontology.eil.utoronto.ca/5087/Change/Manifestation> ;
                                                                   rdfs:range <http://ontology.eil.utoronto.ca/5087/Change/Manifestation> .


###  http://ontology.eil.utoronto.ca/5087/Change/precedesManifestation
<http://ontology.eil.utoronto.ca/5087/Change/precedesManifestation> rdf:type owl:ObjectProperty ;
                                                                    rdfs:subPropertyOf <http://ontology.eil.utoronto.ca/5087/Change/ChangeOntologyProperty> ;
                                                                    rdfs:domain <http://ontology.eil.utoronto.ca/5087/Change/FirstManifestation> ;
                                                                    rdfs:range [ owl:intersectionOf ( <http://ontology.eil.utoronto.ca/5087/Change/Manifestation>
                                                                                                      [ rdf:type owl:Class ;
                                                                                                        owl:complementOf <http://ontology.eil.utoronto.ca/5087/Change/FirstManifestation>
                                                                                                      ]
                                                                                                    ) ;
                                                                                 rdf:type owl:Class
                                                                               ] ;
                                                                    rdfs:comment "The relationship between the FirstManifestation and all subsequent Manifestations." .


###  http://ontology.eil.utoronto.ca/5087/SpatialLoc/SpatialLocOntologyProperty
<http://ontology.eil.utoronto.ca/5087/SpatialLoc/SpatialLocOntologyProperty> rdf:type owl:ObjectProperty ;
                                                                             rdfs:comment "Created for organizational purposes, to identify the object properties imported by geoSPARQL." .


###  http://ontology.eil.utoronto.ca/5087/SpatialLoc/associatedLocation
<http://ontology.eil.utoronto.ca/5087/SpatialLoc/associatedLocation> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf <http://ontology.eil.utoronto.ca/5087/SpatialLoc/SpatialLocOntologyProperty> ;
                                                                     rdfs:range <http://www.opengis.net/ont/geosparql#Feature> ;
                                                                     rdfs:comment "Describes an approximate location of some object (spatial or non-spatial). For example, a Building has an actual location (represented by it's footprint or 3d massing), but it may also have an associated location(s) of some specific point coordinate. Recurring events may not have an actual location, but may be associated with some location (at which their occurrences are likely to occur)." .


