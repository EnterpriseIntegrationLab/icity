                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/5087/Activity/causedBy"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/5087/Activity/Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/5087/Activity/effectOf"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/5087/Activity/Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
