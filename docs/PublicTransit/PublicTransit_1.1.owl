    <!-- http://ontology.eil.utoronto.ca/icity/MonetaryValue/1.0/MonetaryValue -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/MonetaryValue/1.0/MonetaryValue"/>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/AccessMethod -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/AccessMethod">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/hasMonetaryCost"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/MonetaryValue/1.0/MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/validFor"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2006/time#DurationDescription"/>
                            <rdf:Description rdf:about="http://www.wurvoc.org/vocabularies/om-1.8/Quantity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>An Access Method is the means of access to a Line
An AccessMethod has a Monetary Value.
An AccessMethod may be valid for a specific distance or time.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/Fare -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/Fare">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/AccessMethod"/>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/Pass -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/Pass">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/AccessMethod"/>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/Route -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/Route">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/Change/1.1/manifestationOf"/>
                        <owl:someValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/RoutePD"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/Change/1.1/manifestationOf"/>
                        <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/RoutePD"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/Change/1.1/Manifestation"/>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/Change/1.1/existsAt"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/Mereology/1.0//1.0#contains"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteLink"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/hasSection"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route consists of a series of Route Sections.
A Route has some directionality.
A Route is executed by various Vehicles at different points in time.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteId -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteId">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteLink -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteLink">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/beginsAtStop"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/endsAtStop"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Link begins and ends at a Stop Point.
A Route Link operates on an Arc.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/RoutePD -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/RoutePD">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/Change/1.1/hasManifestation"/>
                        <owl:someValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/Route"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/Change/1.1/hasManifestation"/>
                        <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/Route"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/Change/1.1/TimeVaryingConcept"/>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/Change/1.1/existsAt"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#Interval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/hasRouteId"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteId"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Routes change over time. RoutePD captures the entire entity of a route, i.e. the route perdurant.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteSection -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteSection">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/Mereology/1.0//1.0#contains"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteLink"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Section is part of a Route and consists of Route Links.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteTimetable -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteTimetable">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/arrivalTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/timetableFor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/Route"/>
                            <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/RouteLink"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/travelTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DurationDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Route Timetable represents schedule information for a particular Route, or Route Link.
A RouteTimetable has an expected travel time (Duration) for the Route, or Route Link.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/StopPoint -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/StopPoint">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/SpatialLoc/1.0/hasLocation"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/icity/SpatialLoc/1.0/SpatialFeature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A Stop Point marks the start or end of a Route Link.
A Stop Point has a Location.
A Person may enter or exit the transit vehicle at a Stop Point.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/StopTimetable -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/StopTimetable">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/arrivalTime"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#DateTimeDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/timetableFor"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/icity/PublicTransit/StopPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>A StopTimetable has captures the expected arrival time (Time Instant) for some Stop Point.</dc:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitOntologyThing">
        <rdfs:comment>Added for organizational purposes, to identify all classes defined in the Public Transit ontology.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitSystem -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/icity/PublicTransit/TransitSystem">
