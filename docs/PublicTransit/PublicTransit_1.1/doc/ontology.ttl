###  https://w3id.org/icity/PublicTransit/AccessMethod
:AccessMethod rdf:type owl:Class ;
              rdfs:subClassOf :TransitOntologyThing ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasMonetaryCost ;
                                owl:allValuesFrom <https://w3id.org/icity/MonetaryValue/1.0/MonetaryValue>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :validFor ;
                                owl:allValuesFrom [ rdf:type owl:Class ;
                                                    owl:unionOf ( <http://www.w3.org/2006/time#DurationDescription>
                                                                  <http://www.wurvoc.org/vocabularies/om-1.8/Quantity>
                                                                )
                                                  ]
                              ] ;
              <http://purl.org/dc/elements/1.1/description> """An Access Method is the means of access to a Line
An AccessMethod has a Monetary Value.
An AccessMethod may be valid for a specific distance or time."""^^xsd:string .


###  https://w3id.org/icity/PublicTransit/Fare
:Fare rdf:type owl:Class ;
      rdfs:subClassOf :AccessMethod .


###  https://w3id.org/icity/PublicTransit/Pass
:Pass rdf:type owl:Class ;
      rdfs:subClassOf :AccessMethod .


###  https://w3id.org/icity/PublicTransit/Route
:Route rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty <https://w3id.org/icity/Change/1.1/manifestationOf> ;
                                                    owl:someValuesFrom :RoutePD
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <https://w3id.org/icity/Change/1.1/manifestationOf> ;
                                                    owl:allValuesFrom :RoutePD
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf <https://w3id.org/icity/Change/1.1/Manifestation> ,
                       :TransitOntologyThing ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <https://w3id.org/icity/Change/1.1/existsAt> ;
                         owl:allValuesFrom <http://www.w3.org/2006/time#TemporalEntity>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <https://w3id.org/icity/Mereology/1.0//1.0#contains> ;
                         owl:allValuesFrom :RouteLink
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasSection ;
                         owl:allValuesFrom :RouteSection
                       ] ;
       <http://purl.org/dc/elements/1.1/description> """A Route consists of a series of Route Sections.
A Route has some directionality.
A Route is executed by various Vehicles at different points in time."""^^xsd:string .


###  https://w3id.org/icity/PublicTransit/RouteId
:RouteId rdf:type owl:Class ;
         rdfs:subClassOf :TransitOntologyThing .


###  https://w3id.org/icity/PublicTransit/RouteLink
:RouteLink rdf:type owl:Class ;
           rdfs:subClassOf :TransitOntologyThing ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :beginsAtStop ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :StopPoint
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :endsAtStop ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :StopPoint
                           ] ;
           <http://purl.org/dc/elements/1.1/description> """A Route Link begins and ends at a Stop Point.
A Route Link operates on an Arc."""^^xsd:string .


###  https://w3id.org/icity/PublicTransit/RoutePD
:RoutePD rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty <https://w3id.org/icity/Change/1.1/hasManifestation> ;
                                                      owl:someValuesFrom :Route
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty <https://w3id.org/icity/Change/1.1/hasManifestation> ;
                                                      owl:allValuesFrom :Route
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf <https://w3id.org/icity/Change/1.1/TimeVaryingConcept> ,
                         :TransitOntologyThing ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <https://w3id.org/icity/Change/1.1/existsAt> ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass <http://www.w3.org/2006/time#Interval>
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :hasRouteId ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :RouteId
                         ] ;
         <http://purl.org/dc/elements/1.1/description> "Routes change over time. RoutePD captures the entire entity of a route, i.e. the route perdurant."^^xsd:string .


###  https://w3id.org/icity/PublicTransit/RouteSection
:RouteSection rdf:type owl:Class ;
              rdfs:subClassOf :TransitOntologyThing ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty <https://w3id.org/icity/Mereology/1.0//1.0#contains> ;
                                owl:allValuesFrom :RouteLink
                              ] ;
              <http://purl.org/dc/elements/1.1/description> "A Route Section is part of a Route and consists of Route Links."^^xsd:string .


###  https://w3id.org/icity/PublicTransit/RouteTimetable
:RouteTimetable rdf:type owl:Class ;
                rdfs:subClassOf :TransitOntologyThing ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :arrivalTime ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <http://www.w3.org/2006/time#DateTimeDescription>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :timetableFor ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass [ rdf:type owl:Class ;
                                                owl:unionOf ( :Route
                                                              :RouteLink
                                                            )
                                              ]
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :travelTime ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <http://www.w3.org/2006/time#DurationDescription>
                                ] ;
                <http://purl.org/dc/elements/1.1/description> """A Route Timetable represents schedule information for a particular Route, or Route Link.
A RouteTimetable has an expected travel time (Duration) for the Route, or Route Link."""^^xsd:string .


###  https://w3id.org/icity/PublicTransit/StopPoint
:StopPoint rdf:type owl:Class ;
           rdfs:subClassOf :TransitOntologyThing ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <https://w3id.org/icity/SpatialLoc/1.0/hasLocation> ;
                             owl:allValuesFrom <https://w3id.org/icity/SpatialLoc/1.0/SpatialFeature>
                           ] ;
           <http://purl.org/dc/elements/1.1/description> """A Stop Point marks the start or end of a Route Link.
A Stop Point has a Location.
A Person may enter or exit the transit vehicle at a Stop Point."""^^xsd:string .


###  https://w3id.org/icity/PublicTransit/StopTimetable
:StopTimetable rdf:type owl:Class ;
               rdfs:subClassOf :TransitOntologyThing ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :arrivalTime ;
                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass <http://www.w3.org/2006/time#DateTimeDescription>
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :timetableFor ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :StopPoint
                               ] ;
               <http://purl.org/dc/elements/1.1/description> "A StopTimetable has captures the expected arrival time (Time Instant) for some Stop Point."^^xsd:string .


###  https://w3id.org/icity/PublicTransit/TransitOntologyThing
:TransitOntologyThing rdf:type owl:Class ;
                      rdfs:comment "Added for organizational purposes, to identify all classes defined in the Public Transit ontology."^^xsd:string .


###  https://w3id.org/icity/PublicTransit/TransitSystem
:TransitSystem rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
