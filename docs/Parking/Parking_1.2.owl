    <!-- https://w3id.org/icity/Parking/AccessibilityParkingSpace -->

    <owl:Class rdf:about="https://w3id.org/icity/Parking/AccessibilityParkingSpace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/Parking/ParkingSpace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
