    <!-- https://w3id.org/icity/Parking/ParkingArea -->

    <owl:Class rdf:about="https://w3id.org/icity/Parking/ParkingArea">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/manifestationOf"/>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/icity/Parking/ParkingAreaPD"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/icity/Change/manifestationOf"/>
                        <owl:allValuesFrom rdf:resource="https://w3id.org/icity/Parking/ParkingAreaPD"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/Change/Manifestation"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/icity/Parking/ParkingOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Parking/associatedLocation"/>
                <owl:someValuesFrom rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Parking/ownedBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/organization.owl#Organization"/>
                            <rdf:Description rdf:about="https://w3id.org/icity/Person/Person"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Mereology/hasProperPart"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/icity/Parking/ParkingArea"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Parking/hasEVCharger"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/icity/Parking/EVCharger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Parking/hasParkingPolicy"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/icity/Parking/ParkingPolicy"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Parking/hasParkingService"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/icity/Parking/ParkingService"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/icity/Parking/hasVehicleCapacity"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/icity/OM/CapacitySize"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/govstat.owl#cardinality_of"/>
                                <owl:allValuesFrom>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/GCI/Foundation/GCI-Foundation-v2.owl#defined_by"/>
                                        <owl:allValuesFrom rdf:resource="https://w3id.org/icity/Vehicle/Vehicle"/>
                                    </owl:Restriction>
