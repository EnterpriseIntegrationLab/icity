                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty <http://schema.org/spouse> ;
                                                                        owl:allValuesFrom <http://ontology.eil.utoronto.ca/icity/Person/Person>
                                                                      ] ,
                                                                      [ rdf:type owl:Restriction ;
