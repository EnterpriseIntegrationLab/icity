    <!-- http://ontology.eil.utoronto.ca/cdm/Change/TimeVaryingConcept -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/cdm/Change/TimeVaryingConcept">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/hasManifestation"/>
                        <owl:someValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/Manifestation"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/hasManifestation"/>
                        <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/Manifestation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/cdm/Change/ChangeOntologyThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
