                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/5087/RecurringEvent/hasMonth"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/5087/RecurringEvent/Month"/>
            </owl:Restriction>
        </rdfs:subClassOf>
