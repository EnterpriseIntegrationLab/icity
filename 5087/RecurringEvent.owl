            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/5087/RecurringEvent/endDayOfWeek"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/5087/RecurringEvent/DayOfWeek"/>
            </owl:Restriction>
