    <!-- https://standards.iso.org/iso-iec/5087/-1/ed-1/en/ontology/RecurringEvent/YearlyRecurringEvent -->

    <owl:Class rdf:about="https://standards.iso.org/iso-iec/5087/-1/ed-1/en/ontology/RecurringEvent/YearlyRecurringEvent">
        <rdfs:subClassOf rdf:resource="https://standards.iso.org/iso-iec/5087/-1/ed-1/en/ontology/RecurringEvent/RecurringEvent"/>
