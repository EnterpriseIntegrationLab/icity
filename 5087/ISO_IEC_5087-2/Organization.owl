    <!-- https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/Organization/BusinessEstablishment -->

    <owl:Class rdf:about="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/Organization/BusinessEstablishment">
        <rdfs:subClassOf rdf:resource="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/Organization/CityOrgOntologyThing"/>
        <rdfs:subClassOf>
