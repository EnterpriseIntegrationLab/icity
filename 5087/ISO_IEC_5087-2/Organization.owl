                <owl:onProperty rdf:resource="https://standards.iso.org/iso-iec/5087/-1/ed-1/en/ontology/SpatialLoc/hasLocation"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://standards.iso.org/iso-iec/5087/-1/ed-1/en/ontology/SpatialLoc/Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>Business Establishment: A Business establishment is a physical location where a Firm conducts business.
A Business Establishment has a Location and may have an address.</dc:description>
    </owl:Class>
    


