    <owl:Ontology rdf:about="http://ontology.eil.utoronto.ca/5087/2/Organization/">
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/1/Activity.owl"/>
        <owl:imports rdf:resource="http://ontology.eil.utoronto.ca/5087/1/Change.owl"/>
