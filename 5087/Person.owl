    <!-- http://ontology.eil.utoronto.ca/5087/Person/Sex -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/5087/Person/Sex">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/5087/Person/female"/>
                    <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/5087/Person/male"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/5087/Person/PersonOntologyThing"/>
    </owl:Class>
    


