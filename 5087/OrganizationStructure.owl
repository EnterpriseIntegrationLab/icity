    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/5087/OrganizationStructure/Organization">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/5087/Agent/Agent"/>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/5087/OrganizationStructure/OrganizationStructureOntologyThing"/>
