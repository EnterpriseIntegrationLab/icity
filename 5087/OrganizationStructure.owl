    <!-- http://ontology.eil.utoronto.ca/5087/OrganizationStructure/OrganizationalUnit -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/5087/OrganizationStructure/OrganizationalUnit">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/5087/OrganizationStructure/Organization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/org#unitOf"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/5087/OrganizationStructure/Organization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">●	OrganizationalUnit: An Organization, such as a department, that is part of a larger Organization, but is recognized only in the context of the Organization.  Not regarded as a legal entity in its own right.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/5087/OrganizationStructure/Post -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/5087/OrganizationStructure/Post">
